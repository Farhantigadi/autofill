{
  "manifest_version": 3,
  "name": "Test Autofill",
  "version": "1.0.0",
  "description": "Simple test extension",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "popup.html"
  }
}